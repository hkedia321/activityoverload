<div class="container">
  <br><br><br><br>
  <h3>Users</h3>
  <table class="table">
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Title</th>
      <th>Email</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
    <% _.each(users,function(user){%>
    <tr data-id="<%=user.id%>" data-model="user">
      <td><%=user.id%></td>
      <td><%=user.name%></td>
      <td><%=user.title%></td>
      <td><%=user.email%></td>
      <td><a href="/users/show/<%=user.id%>" class="btn btn-sm btn-primary">Show</a> </td>
      <td><a href="/users/edit/<%=user.id%>" class="btn btn-sm btn-warning">Edit</a> </td>
      <td><a href="/users/destroy/<%=user.id%>" class="btn btn-sm btn-danger">Delete get</a> </td>
      <td>
        <form action="/users/destroy/<%=user.id%>" method="POST">
          <input type="hidden" name="_method" value="delete">
          <input type="submit" class="btn btn-danger" value="delete post">
          <input type="hidden" name="_csrf" value="<%=_csrf%>">
        </form>
      </td>
    </tr>
    <%})%>
  </table>
</div>
